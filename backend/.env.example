# 应用配置
NODE_ENV=development
PORT=8080
LOG_LEVEL=debug

# 数据库配置
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=aftersales
DATABASE_USER=admin
DATABASE_PASSWORD=admin123
DATABASE_URL=postgresql://admin:admin123@localhost:5432/aftersales

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379

# JWT配置
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# 外部服务
FEISHU_APP_ID=
FEISHU_APP_SECRET=
FEISHU_WEBHOOK_URL=

# AI服务
AI_SERVICE_URL=
AI_SERVICE_API_KEY=
AI_SERVICE_PROVIDER=openai

# AgentScope 服务（FastAPI）
AGENTSCOPE_URL=http://localhost:5000

# TaxKB知识库
# 是否启用TaxKB集成（生产环境设为true）
TAXKB_ENABLED=false
# TaxKB服务地址（API v3端点）
TAXKB_BASE_URL=http://localhost:8000/api/v3
# TaxKB API密钥（从TaxKB管理后台获取）
TAXKB_API_KEY=test_api_key
# 请求超时时间（毫秒）
TAXKB_TIMEOUT=30000
# 缓存配置（可选）
TAXKB_CACHE_ENABLED=true
TAXKB_CACHE_TTL=300
TAXKB_CACHE_MAX_SIZE=100
# 重试配置（可选）
TAXKB_RETRY_MAX_ATTEMPTS=3
TAXKB_RETRY_BACKOFF=1000

# 监控
SENTRY_DSN=
PROMETHEUS_ENABLED=true
